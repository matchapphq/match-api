{
  "name": "match-api",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun --watch index.ts",
    "db:setup": "bun run src/config/setup.db.ts",
    "db:migrate": "bun run db:setup && drizzle-kit migrate",
    "db:generate": "drizzle-kit generate",
    "db:push": "bun run db:setup && drizzle-kit push",
    "db:seed": "bun run scripts/seed-sports.ts && bun run scripts/seed-data.ts",
    "start": "bun run index.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/pg": "^8.15.5",
    "@types/qrcode": "^1.5.6",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.38.0",
    "tsx": "^4.21.0"
  },
  "peerDependencies": {
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.7.2",
    "@neondatabase/serverless": "^1.0.2",
    "axios": "^1.11.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.45.1",
    "hono": "^4.9.6",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "stripe": "^20.1.0",
    "zod": "^4.1.5"
  }
}
